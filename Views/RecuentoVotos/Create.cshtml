@model ComputosPJ.Models.RecuentoVoto

@{
    ViewBag.Title = "Create";
}




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Create RecuentoVoto</h5>
                </div>
                <div class="ibox-content">

				@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.LabelFor(model => model.CantidadVotos, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.CantidadVotos)
                @Html.ValidationMessageFor(model => model.CantidadVotos)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.EnlaceEleccionTipoCategoriaListaID, "EnlaceEleccionTipoCategoriaListaID", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
				@Html.DropDownListFor(model => model.EnlaceEleccionTipoCategoriaListaID, (IEnumerable<SelectListItem>) ViewBag.EnlaceEleccionTipoCategoriaListaID, new { @class = "form-control m-b" })                
                @Html.ValidationMessageFor(model => model.EnlaceEleccionTipoCategoriaListaID)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.FechaCreacion, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.FechaCreacion)
                @Html.ValidationMessageFor(model => model.FechaCreacion)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.FechaActualizacion, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.FechaActualizacion)
                @Html.ValidationMessageFor(model => model.FechaActualizacion)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.UsuarioCreacionID, "UsuarioCreacionID", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
				@Html.DropDownListFor(model => model.UsuarioCreacionID, (IEnumerable<SelectListItem>) ViewBag.UsuarioCreacionID, new { @class = "form-control m-b" })                
                @Html.ValidationMessageFor(model => model.UsuarioCreacionID)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.UsuarioActualizacionID, "UsuarioActualizacionID", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
				@Html.DropDownListFor(model => model.UsuarioActualizacionID, (IEnumerable<SelectListItem>) ViewBag.UsuarioActualizacionID, new { @class = "form-control m-b" })                
                @Html.ValidationMessageFor(model => model.UsuarioActualizacionID)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.MesaID, "MesaID", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
				@Html.DropDownListFor(model => model.MesaID, (IEnumerable<SelectListItem>) ViewBag.MesaID, new { @class = "form-control m-b" })                
                @Html.ValidationMessageFor(model => model.MesaID)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Crear" class="btn btn-primary" />
				@Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-white"})
            </div>
        </div>
    </div>
}

                </div>
            </div>
        </div>
    </div>
 </div>

 

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
